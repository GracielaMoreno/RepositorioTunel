<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Temp_list_pago"
    android:orientation="vertical">

    <RelativeLayout
        android:id="@+id/relativeLayout2"
        android:layout_width="wrap_content"
        android:layout_height="75dp"
        android:layout_alignParentTop="true"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/txtCampo2"
            android:layout_width="110dp"
            android:layout_height="22dp"
            android:layout_below="@+id/txtCampo8"
            android:background="@color/colorApp"
            android:fontFamily="@font/gothambook"
            android:gravity="center"
            android:text="Tag"
            android:textColor="@color/fondo"
            android:textSize="15dp" />

        <TextView
            android:id="@+id/txtCampo9"
            android:layout_width="110dp"
            android:layout_height="25dp"
            android:layout_below="@+id/txtCampo2"
            android:background="@color/colorApp"
            android:fontFamily="@font/gothambook"
            android:gravity="center"
            android:text="Placa"
            android:textColor="@color/fondo"
            android:textSize="15dp" />

        <TextView
            android:id="@+id/txtCampo8"
            android:layout_width="110dp"
            android:layout_height="wrap_content"
            android:layout_alignParentTop="true"
            android:layout_marginTop="0dp"
            android:background="@color/colorApp"
            android:fontFamily="@font/gothambook"
            android:gravity="center"
            android:text="No. Tag"
            android:textColor="@color/fondo"
            android:textSize="15dp" />

        <TextView
            android:id="@+id/txtVacio"
            android:layout_width="5dp"
            android:layout_height="match_parent"
            android:layout_toRightOf="@+id/txtCampo2" />

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_toRightOf="@+id/txtVacio"
            android:orientation="vertical">

            <TextView
                android:id="@+id/txtCampo3"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_alignParentTop="true"
                android:fontFamily="@font/robotolight"
                android:text="Concepto"
                android:textSize="14dp"
                android:textColor="@color/texto" />

            <TextView
                android:id="@+id/txtCampo4"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true"
                android:layout_below="@+id/txtCampo3"
                android:fontFamily="@font/robotolight"
                android:text="Valores"
                android:textSize="14dp"
                android:textColor="@color/texto" />

            <TextView
                android:id="@+id/txtCampo5"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true"
                android:layout_below="@+id/txtCampo4"
                android:fontFamily="@font/robotolight"
                android:text="Vehiculo"
                android:textSize="14dp"
                android:textColor="@color/texto" />

            <TextView
                android:id="@+id/txtCampo10"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/txtCampo5"
                android:fontFamily="@font/robotolight"
                android:text="No. Factura"
                android:textSize="14dp"
                android:textColor="@color/texto" />
        </RelativeLayout>
    </RelativeLayout>

    <TextView
        android:id="@+id/txtVacio1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/relativeLayout2"
        android:layout_alignStart="@+id/relativeLayout2"
        android:layout_below="@+id/relativeLayout2" />
</RelativeLayout>








