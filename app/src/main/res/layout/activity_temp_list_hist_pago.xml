<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Temp_list_hist_pago"
    android:orientation="vertical"
    >

    <RelativeLayout
        android:id="@+id/relativeLayout2"
        android:layout_width="wrap_content"
        android:layout_height="104dp"
        android:layout_alignParentTop="true"

        android:orientation="horizontal">

        <TextView
            android:id="@+id/txtCampo2"
            android:layout_width="133dp"
            android:layout_height="wrap_content"
            android:layout_below="@+id/txtCampo8"
            android:gravity="center"
            android:text="Tag"
            android:textSize="15dp"
            android:fontFamily="@font/gothambook"
            android:textColor="@color/fondo"
            android:background="@color/colorApp"/>

        <TextView
            android:id="@+id/txtCampo9"
            android:layout_width="133dp"
            android:layout_height="39dp"
            android:layout_below="@+id/txtCampo2"
            android:gravity="center"
            android:text="Placa"
            android:textSize="15dp"
            android:fontFamily="@font/gothambook"
            android:textColor="@color/fondo"
            android:background="@color/colorApp"/>

        <TextView
            android:id="@+id/txtCampo8"
            android:layout_width="133dp"
            android:layout_height="38dp"
            android:layout_alignParentTop="true"
            android:gravity="center"
            android:text="No. Tag"
            android:textSize="17dp"
            android:fontFamily="@font/gothambook"
            android:textColor="@color/fondo"
            android:background="@color/colorApp"/>

        <TextView
            android:id="@+id/txtVacio"
            android:layout_width="5dp"
            android:layout_height="match_parent"
            android:layout_toRightOf="@+id/txtCampo2" />

        <RelativeLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_toRightOf="@+id/txtVacio"
            android:orientation="vertical">

            <TextView
                android:id="@+id/txtCampo3"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_alignParentTop="true"
                android:fontFamily="@font/robotolight"
                android:text="Cantidad"
                android:textSize="14dp"
                android:textColor="@color/texto" />

            <TextView
                android:id="@+id/txtCampo4"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true"
                android:layout_below="@+id/txtCampo3"
                android:fontFamily="@font/robotolight"
                android:text="Fecha"
                android:textSize="14dp"
                android:textColor="@color/texto" />

            <TextView
                android:id="@+id/txtCampo5"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true"
                android:layout_below="@+id/txtCampo4"
                android:fontFamily="@font/robotolight"
                android:text="Medio de pago"
                android:textSize="14dp"
                android:textColor="@color/texto" />

            <TextView
                android:id="@+id/txtCampo10"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/txtCampo5"
                android:fontFamily="@font/robotolight"
                android:text="Estado de recarga"
                android:textColor="@color/texto"
                android:textSize="14dp"/>

            <TextView
                android:id="@+id/txtCampo11"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/txtCampo10"
                android:fontFamily="@font/robotolight"
                android:text="Decripcion"
                android:textSize="14dp"
                android:textColor="@color/texto" />
        </RelativeLayout>
    </RelativeLayout>

</RelativeLayout>








