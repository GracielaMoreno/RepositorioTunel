<resources xmlns:tools="http://schemas.android.com/tools">

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Base.Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>

    </style>
    <style name="CustonTheme" parent="Base.Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">#0f5598</item>
        <item name="colorPrimaryDark">#0f5598</item>
        <item name="colorAccent">@color/colorAccent</item>

    </style>

    <style name="MyApp.PopupMenu" parent="Base.Theme.AppCompat.Light.DarkActionBar">
        <item name="android:popupBackground">@drawable/menu_dropdown_panel</item>
    </style>

    <style name="CustomDialog" parent="android:Theme.DeviceDefault.Dialog">
        <item name="android:windowIsTranslucent">false</item>
        <item name="android:windowBackground">@color/texto</item>
        <item name="android:titleTextStyle">@font/roboto_regular</item>
        <item name="android:textColor">@color/fondo</item>
        <item name="android:popupBackground">@color/colorAccent</item>
        <item name="android:tint">@color/colorAccent</item>
    </style>
    <style name="Appmenu" parent="Theme.AppCompat.NoActionBar">
        <item name="android:background">@color/colorAccent</item>
        <item name="android:windowBackground">@color/colorAccent</item>
        <item name="android:colorBackground">@color/colorAccent</item>
`       <item name="android:menuCategory">@color/colorAccent</item>
        <item name="android:tint">@color/colorAccent</item>
        <item name="colorPrimary">@color/colorAccent</item>
        <item name="colorPrimaryDark">@color/colorAccent</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>
</resources>
